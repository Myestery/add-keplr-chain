{
  "name": "add-keplr-chain",
  "version": "0.0.2",
  "description": "Adds chains to Keplr Wallet",
  "main": "./dist/add-keplr-chain.cjs",
  "keywords": [
    "keplr",
    "cosmos",
    "blockchain",
    "wallet",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:myestery/add-keplr-chain.git"
  },
  "module": "./dist/add-keplr-chain.mjs",
  "exports": {
    ".": {
      "require": "./dist/add-keplr-chain.cjs",
      "import": "./dist/add-keplr-chain.mjs"
    }
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts && tsc",
    "test": "vitest",
    "tsc": "tsc",
    "format:scripts": "prettier . --write",
    "format:styles": "stylelint ./**/*.{css,scss} --fix",
    "format": "npm run format:scripts && npm run format:styles"
  },
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ],
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0",
    "@keplr-wallet/types": "^0.11.51"
  },
  "devDependencies": {
    "@keplr-wallet/types": "^0.11.51",
    "@types/react": ">=16.0.0",
    "dts-bundle-generator": "^9.5.1",
    "react": ">=16.0.0",
    "typescript": "^4.9.5",
    "vite": "^5.3.5",
    "vitest": "^2.0.5"
  },
  "author": "myestery",
  "license": "MIT"
}
